    "rowsha": "uWURPmF6qz82b2uQB8IWTiHJsxdfVFWsMHZHGVcf6PM=",
    "originContent": "pip install git+https://github.com/cclgroupltd/ccl_chromium_reader.git",
    "translatedContent": "pip install git+https://github.com/cclgroupltd/ccl_chromium_reader.git"
  },
