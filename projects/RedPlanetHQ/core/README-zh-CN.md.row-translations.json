    "originContent": "<img width=\"6048\" height=\"3428\" alt=\"benchmark\" src=\"https://github.com/user-attachments/assets/2e5fdac5-02ed-4d00-9312-c21d09974e1f\" />",
    "translatedContent": "<img width=\"6048\" height=\"3428\" alt=\"benchmark\" src=\"https://github.com/user-attachments/assets/2e5fdac5-02ed-4d00-9312-c21d09974e1f\" />"
