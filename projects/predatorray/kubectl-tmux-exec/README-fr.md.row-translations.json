    "rowsha": "x1sTlFD9ZhFWFh/fSAdE/GWky8PyoFJ0iYEemLyuWHk=",
    "originContent": "kubectl tmux-exec -l foo=bar /bin/bash",
    "translatedContent": "kubectl tmux-exec -l foo=bar /bin/bash"
  },
