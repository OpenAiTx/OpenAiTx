    "translatedContent": "  Lens.key(:main_widget),"
  },
  {
    "row": 59,
    "rowsha": "kCYo1Djmg258UMTuLcay6iMopB/TIu9PBxu/+sy2q9Y=",
    "originContent": "  Lens.key(:other_widgets) |> Lens.all",
    "translatedContent": "  Lens.key(:other_widgets) |> Lens.all"
