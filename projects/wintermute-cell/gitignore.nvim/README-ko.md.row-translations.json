    "translatedContent": "            height = 0.3,"
  },
  {
    "row": 137,
    "rowsha": "Vev0I6JA3QMGP90IK+NqJ8jUIp+xXKTLNc812UNIYGY=",
    "originContent": "        },",
    "translatedContent": "        },"
  },
  {
    "row": 138,
    "rowsha": "PZrc1vXBzmwlnOJUSPrLKNjXBMgUz13DUEYPJlCNfiA=",
    "originContent": "        actions = {",
    "translatedContent": "        actions = {"
