    "rowsha": "9cK13kxEeA/FJfZrdKdyd/lp3xgn2r8NuwxPAZc1M+s=",
    "originContent": "require('nvim-treesitter.configs').setup {",
    "translatedContent": "require('nvim-treesitter.configs').setup {"
  },
