    "originContent": "    enable = true,",
    "translatedContent": "    enable = true,"
  },
  {
    "row": 27,
    "rowsha": "MoihNso+fIVk/t0zIqMIstBF8nPbUFXi4q6h+C0WIY4=",
    "originContent": "  },",
